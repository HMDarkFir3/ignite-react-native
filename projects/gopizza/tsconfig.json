{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@dtos/*": ["./src/dtos/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@routes/*": ["./src/routes/*"],
      "@screens/*": ["./src/screens/*"],
      "@storages/*": ["./src/storages/*"],
      "@themes/*": ["./src/themes/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "types": ["styled-components-react-native"]
}
